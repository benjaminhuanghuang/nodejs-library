{
    "name": "nodejs-library",
    "version": "0.0.0",
    "private": true,
    "engines": {
        "node": ">= 7.6.0"
    },
    "scripts": {
        "start": "node ./server.js",
        "dev": "concurrently \"npm run server\" \"npm run assets\"",
        "server": "nodemon ./server.js --ignore public/",
        "assets": "webpack -w --display-max-modules 0",
        "cleardb": "node ./data/clear.js"
    },
    "dependencies": {
        "express": "4.15.3",
        "dotenv": "4.0.0",
        "mongoose": "4.11.3",
        "body-parser": "1.17.2",
        "pug": "2.0.0-rc.2",
        "slugs": "~0.1.3",
        "multer": "~1.3.0",
        "moment": "2.18.1",
        "uuid": "3.1.0",
        "jimp": "0.2.28",
        "express-session":"~1.15.3",
        "connect-mongo":"~1.3.2",
        "connect-flash":"~0.1.1",
        "passport": "0.3.2",
        "passport-local": "1.0.0",
        "passport-local-mongoose": "4.0.0",
        "validator": "7.0.0",
        "express-validator":"~3.2.1",
        "md5": "2.2.1",
        "mongoose-mongodb-errors":"~0.0.2",
        "es6-promisify":"~5.0.0",
        "nodemailer": "3.1.5",
        "crypto":"~0.0.3",
        "juice": "4.0.2",
        "html-to-text":"~3.3.0",
        "axios": "0.16.2"
    },
    "devDependencies": {
        "nodemon": "1.11.0",
        "concurrently": "3.5.0",
        "webpack": "3.3.0",
        "postcss-loader": "1.3.3",
        "sass-loader": "6.0.6",
        "node-sass": "4.5.3",
        "css-loader": "0.28.4",
        "extract-text-webpack-plugin": "3.0.0",
        "autoprefixer": "7.1.2",
        "babel-core": "6.25.0",
        "babel-loader": "7.1.1",
        "babel-preset-es2015": "6.24.1"
    }
}